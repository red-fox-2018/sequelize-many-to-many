<!DOCTYPE html>
<html>
<head>
    <title>Page Title</title>
</head>
<body>
    <style>
    body{
        text-align: center;
        
    }
    table{
        margin: auto;
    }
    </style>

    <h1>Students Data</h1>
    <table border=1>
        <tr>
            <th>id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Actions</th>
            <th>Options</th>
        </tr>
        <% students.forEach(student=>{ %>
            <tr>
                <td><%=student.id%></td>
                <td><%=student.first_name%></td>
                <td><%=student.last_name%></td>
                <td><%=student.email%></td>
                <td><a href="/students/<%= student.id%>/addsubject">Add Subject</a></td>
                <td><a href="">Edit</a> | <a href="">Delete</a></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>